.history-view {
  display: flex;
  flex-direction: column;
  margin: 20px;
}

.history-line-one-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.history-view-others {
  display: flex;
  flex-direction: column;
  margin-top: 5px;
}

.history-left-text {
  font-size: 18px;
  color: #4a4a4a;
  font-family: 'vodafone';
}

.history-detail-text {
  font-size: 14px;
  color: #666666;
  font-family: 'vodafone';
}

.add-icon {
  font-size: 5rpx;
  color: rgb(32, 36, 35);
  margin-right: 10rpx;
}