<view class="vertical-content">
    <top-header title="{{title}}" image="{{image}}" />
    <view a:for="{{messages}}">
        <view class="center-content" style="margin-left:10%;margin-right:10%;margin-top:5%;">
            <text class="messages-text" >{{item}}</text>
        </view>
    </view>
</view>